# 测试环境：只在打包时使用
NODE_ENV=production
VITE_DEV=false
# 请求路径
VITE_BASE_URL='http://localhost:8100'
# 上传路径
VITE_UPLOAD_URL='http://localhost:8100/iris/oss/file/upload'
